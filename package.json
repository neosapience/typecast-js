{
  "name": "typecast-ts",
  "version": "0.1.2",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm test run && npm run build",
    "link:dev": "npm link",
    "clean": "rm -rf lib",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "keywords": ["typecast", "tts", "text-to-speech", "ai", "voice", "speech-synthesis", "typescript"],
  "author": "Neosapience <help@typecast.ai>",
  "license": "Apache-2.0",
  "description": "Official TypeScript SDK for Typecast API - Text-to-Speech with AI voices",
  "repository": {
    "type": "git",
    "url": "https://github.com/neosapience/typecast-ts.git"
  },
  "homepage": "https://typecast.ai",
  "bugs": {
    "url": "https://github.com/neosapience/typecast-ts/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
